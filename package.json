  "scripts": {
    "start": "sls offline start --stage local",
    "test": "echo \"Error: no test specified\" && exit 1",
    "domain": "sls create_domain --stage production",
    "staging-deploy": "sls deploy --stage staging",
    "dev-deploy": "sls deploy --stage development",
    "deploy": "sls deploy --stage production",
    "predomain": "cp configs/stages/production.yml serverless.yml",
    "prestaging-deploy": "cp configs/stages/staging.yml serverless.yml",
    "predeploy": "cp configs/stages/production.yml serverless.yml",
    "predev-deploy": "cp configs/stages/development.yml serverless.yml",
    "prestart": "cp configs/stages/local.yml serverless.yml",
    "postdomain": "rm serverless.yml",
    "poststaging-deploy": "rm serverless.yml",
    "postdeploy": "rm serverless.yml",
    "postdev-deploy": "rm serverless.yml",
    "poststart": "rm serverless.yml",
    "check": "npm-check -u"
  },
  "dependencies": {
    "jsonwebtoken": "^8.1.0",
