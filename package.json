{
  "name": "fi-slseed-api",
  "title": "Fi Slseed API",
  "description": "Serveless Seed API project",
  "author": "Final Development Studio <sorcery@finaldevstudio.com> (http://finaldevstudio.com)",
  "homepage": "https://github.com/FinalDevStudio/fi-slseed-api",
  "main": "npm start",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/FinalDevStudio/fi-slseed-api/issues"
  },
  "directories": {
    "test": "test"
  },
  "contributors": [
    "Santiago G. Mar√≠n <santiago@finaldevstudio.com> (https://github.com/stgogm)",
    "JT Guzman <jtguzman@finaldevstudio.com> (https://github.com/jtguzman)"
  ],
  "keywords": [
    "fi",
    "slseed",
    "serverless",
    "sls",
    "api",
    "seed",
    "project"
  ],
  "group": {
    "title": "Fi Slseed",
    "name": "slseed"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FinalDevStudio/fi-slseed-api.git"
  },
  "os": [
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  },
  "nyc": {
    "reporter": [
      "html"
    ],
    "include": [
      "service"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "deploy": "node deploy-stage && sls deploy --stage $(cat .deploy-stage) && rm .deploy-stage",
    "test:unit": "npm run test:components && npm run test:schemas && npm run test:functions",
    "test:functions": "sls invoke test --stage local --path test/functions",
    "test:components": "NODE_ENV=local mocha test/components",
    "test:schemas": "NODE_ENV=local mocha test/schemas",
    "test": "nyc npm run test:unit",
    "domain": "sls create_domain --stage production",
    "start": "sls offline start --stage local",
    "check": "npm-check -u && npm-check",
    "setup:stack": "node setup/stack",
    "setup:env": "node setup/env",
    "setup": "node setup"
  },
  "dependencies": {
    "ably": "^1.0.20",
    "jsonwebtoken": "^8.4.0",
    "jwk-to-pem": "^2.0.1",
    "mailgun.js": "^2.0.1",
    "moment": "^2.23.0",
    "mongoose": "^5.4.1",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "amazon-cognito-identity-js-node": "0.0.3",
    "aws-sdk": "^2.382.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "fi-is": "^1.3.1",
    "inquirer": "^6.2.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "ora": "^3.0.0",
    "serverless": "^1.35.1",
    "serverless-domain-manager": "^2.6.10",
    "serverless-dotenv-plugin": "^2.0.1",
    "serverless-mocha-plugin": "git+https://github.com/nordcloud/serverless-mocha-plugin.git",
    "serverless-offline": "^3.31.3",
    "serverless-plugin-split-stacks": "^1.7.0",
    "slug": "^0.9.3",
    "titleize": "^1.0.1",
    "walk-sync": "^1.0.1",
    "yargs": "^12.0.5"
  }
}
